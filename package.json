{
  "name": "vitepress-demo-mondaylab",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs:dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" vitepress dev docs --host 0.0.0.0 --port 5173",
    "docs:dev:fast": "cross-env NODE_OPTIONS=\"--max-old-space-size=2048\" vitepress dev docs --host 0.0.0.0 --port 5173 --force",
    "docs:build": "vitepress build docs",
    "docs:build:optimized": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --max-semi-space-size=512\" vitepress build docs",
    "docs:build:safe": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --max-semi-space-size=512\" vitepress build docs --mode production",
    "docs:preview": "vitepress preview docs",
    "predeploy": "pnpm run docs:build:safe",
    "deploy": "gh-pages -d docs/.vitepress/dist -r git@github.com:NanChen042/docs.git -b gh-pages",
    "deploy:retry": "pnpm run predeploy && pnpm run deploy",
    "media:exclude": "node scripts/manage-media.js exclude",
    "media:include": "node scripts/manage-media.js include",
    "commit:code": "pnpm run media:exclude && echo '✅ 现在可以提交代码了: git add . && git commit -m \"feat: 初始化项目结构和代码\"'",
    "commit:media": "pnpm run media:include && echo '✅ 现在可以提交媒体文件了: git add . && git commit -m \"feat: 添加项目媒体文件\"'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "canvas-confetti": "^1.9.3",
    "cross-env": "^10.0.0",
    "gh-pages": "^6.3.0",
    "less": "^4.4.0",
    "sass": "^1.69.7",
    "vitepress": "1.0.0-rc.36",
    "vue": "^3.4.8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@lottiefiles/dotlottie-vue": "^0.9.0",
    "@mdit-vue/shared": "^2.0.0",
    "element-plus": "^2.5.0",
    "tlbs-map-vue": "^1.3.1"
  }
}
